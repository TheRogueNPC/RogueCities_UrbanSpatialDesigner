  "architectural_considerations": {
    "viewport_index_system": {
      "design_principle": "If UI needs data not in the index, the index is wrong — not the UI",
      "purpose": "Flat, cache-friendly table for hover, selection, inspection, outlining, mode filtering",
      "struct_definition": {
        "name": "VpProbeData",
        "members": [
          {
            "name": "kind",
            "type": "VpEntityKind",
            "size": "uint8_t"
          },
          {
            "name": "id",
            "type": "uint32_t",
            "description": "Stable per-generation ID (FVA/SIV backed)"
          },
          {
            "name": "parent",
            "type": "uint32_t",
            "description": "Index into viewport_index"
          },
          {
            "name": "first_child",
            "type": "uint32_t",
            "description": "Child span start"
          },
          {
            "name": "child_count",
            "type": "uint16_t"
          },
          {
            "name": "axiom_id",
            "type": "uint32_t",
            "description": "Causal lineage link (stable id)"
          },
          {
            "name": "road_id",
            "type": "uint32_t"
          },
          {
            "name": "district_id",
            "type": "uint32_t"
          },
          {
            "name": "block_id",
            "type": "uint32_t"
          },
          {
            "name": "slope",
            "type": "float",
            "category": "cached_metric"
          },
          {
            "name": "flood",
            "type": "float",
            "category": "cached_metric"
          },
          {
            "name": "aesp",
            "type": "float[4]",
            "category": "cached_metric"
          },
          {
            "name": "frontage_family",
            "type": "uint8_t"
          },
          {
            "name": "afg_preset",
            "type": "uint8_t"
          },
          {
            "name": "afg_band",
            "type": "float"
          },
          {
            "name": "road_hierarchy",
            "type": "uint8_t"
          },
          {
            "name": "zone_mask",
            "type": "uint8_t"
          },
          {
            "name": "label",
            "type": "char[32]",
            "example": "Road #42"
          }
        ],
        "performance_analysis": {
          "current_size_bytes": 88,
          "cache_line_64_fit": 0.72,
          "cache_line_128_fit": 1.45,
          "optimization_recommendation": "Don't optimize prematurely - memory not a bottleneck even for metropolis (6 MB)"
        }
      },
      "hover_select_path": "auto &e = viewport_index[hit_index]; draw_probe(e);",
      "rule": "No pointer chasing. No generator calls. No allocations."
    },
    
    "editor_panel_pattern": {
      "interface_definition": {
        "name": "IEditorPanel",
        "methods": [
          "OnSelectionChanged(const Selection& sel)",
          "OnEditorStateChanged(EditorState newState)",
          "Render(float dt)",
          "IsDirty() const",
          "OnEnable() [optional]",
          "OnDisable() [optional]",
          "OnProjectLoaded() [optional]"
        ]
      },
      "communication_considerations": [
        "Panel state persistence (save panel layouts)",
        "Panel communication via event bus (avoid tight coupling)"
      ]
    },
    
    "spatial_partitioning": {
      "purpose": [
        "Viewport culling (only render visible entities)",
        "Selection queries (only test nearby objects)",
        "Update optimization (dirty rectangles)"
      ],
      "recommendation": "Use existing grid structures + quadtree/octree for fast spatial queries"
    }
  },
  
  "performance_mandates": {
    "10ms_rule": {
      "description": "Any editor operation >10ms must go through RogueWorker or feel laggy",
      "applies_to": "HFSM state transitions",
      "enforcement_question": "How to enforce? Need instrumentation in HFSM transitions?"
    },
    
    "context_aware_threading": {
      "rule": "Enable threading when axiom_count * district_count > threshold",
      "default_threshold": 100,
      "code_example": "if (axiom_count * district_count > 100) { use_rogue_worker(); } else { run_single_threaded(); }",
      "rationale": "Avoids threading overhead for small cities while enabling parallelism for complex ones"
    },
    
