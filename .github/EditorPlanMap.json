  "required_implementations": {
    "critical_priority": [
      {
        "name": "Unified Property Editor/Inspector",
        "priority": "BLOCKING",
        "current_gap": "Context-sensitive property grid missing",
        "requirements": [
          "Automatically display editable properties for selected entity",
          "Reduce UI clutter by showing only relevant properties",
          "Consistent editing interface across all entity types",
          "Undo/redo integration at property level",
          "Batch editing support for multiple selections"
        ],
        "implementation_pattern": {
          "language": "cpp",
          "class": "PropertyEditor",
          "methods": [
            "Render(SelectionContext& selection)",
            "PropertyGrid<T>(const char* label, T& obj)"
          ],
          "approach": "manual_property_grids",
          "rationale": "C++ lacks native reflection; manual grids are explicit, typesafe, and customizable"
        },
        "roundtable_consensus": "Do NOT attempt C++ reflection - use manual property grids"
      },
      
      {
        "name": "Road Network Editor (Interactive)",
        "priority": "CRITICAL",
        "current_gap": "Road generation exists, but interactive editing missing",
        "features_needed": [
          "Vertex manipulation (drag intersection points)",
          "Edge splitting/merging",
          "Road type conversion (arterial → boulevard)",
          "Curve editing for organic road shapes"
        ],
        "mvp_justification": "Roads are system backbone - users need fine control for fixing intersections, adding shortcuts, creating ramps, adjusting curves",
        "implementation_pattern": {
          "language": "cpp",
          "class": "RoadNetworkEditor",
          "base_class": "IEditorPanel",
          "edit_modes": [
            "Select",
            "AddVertex",
            "SplitEdge",
            "Merge",
            "Convert"
          ],
          "methods": [
            "HandleViewportClick(Vec2 worldPos)",
            "HandleVertexDrag(RoadVertex* v, Vec2 delta)",
            "DrawGizmos()"
          ]
        }
      },
      
      {
        "name": "Timeline/History Editor (Undo System)",
        "priority": "BLOCKING",
        "current_gap": "No undo/redo implementation found",
        "requirements": [
          "Command pattern for all mutations",
          "Visual timeline scrubbing",
          "Branch/fork support for what-if scenarios"
        ],
        "mvp_requirement": "At minimum, implement basic undo/redo for axiom placement",
        "implementation_pattern": {
          "language": "cpp",
          "classes": [
            {
              "name": "ICommand",
              "methods": [
                "Execute()",
                "Undo()",
                "Redo()",
                "GetDescription() const"
              ]
            },
            {
              "name": "CommandHistory",
              "members": [
                "std::vector<std::unique_ptr<ICommand>> m_history",
                "size_t m_current_index"
              ],
              "methods": [
                "Execute(std::unique_ptr<ICommand> cmd)",
                "Undo()",
                "Redo()",
                "CanUndo() const",
                "CanRedo() const"
              ]
            },
            {
              "name": "PlaceAxiomCommand",
              "base_class": "ICommand",
              "example": true
            }
          ]
        },
        "roundtable_consensus": "Move from Critical to BLOCKING - implement FIRST before any interactive editing"
      },
      
